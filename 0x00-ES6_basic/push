#!/bin/bash

# Variables
BRANCH_NAME="main"  # Pushing to main branch
COMMIT_MESSAGE="Your commit message here"  # Add your commit message

# Add all files
git add .

# Commit the changes
git commit -m "$COMMIT_MESSAGE"

# Push the changes to the main branch
git push origin "$BRANCH_NAME"

# Success message
echo "Changes pushed to GitHub on branch $BRANCH_NAME"
